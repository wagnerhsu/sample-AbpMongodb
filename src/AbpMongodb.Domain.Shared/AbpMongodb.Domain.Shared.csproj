<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>AbpMongodb</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Volo.Abp.Identity.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.IdentityServer.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.BackgroundJobs.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.AuditLogging.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.TenantManagement.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.FeatureManagement.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.PermissionManagement.Domain.Shared" Version="4.1.0" />
    <PackageReference Include="Volo.Abp.SettingManagement.Domain.Shared" Version="4.1.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\AbpMongodb\*.json" />
    <Content Remove="Localization\AbpMongodb\*.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="5.0.1" />
  </ItemGroup>

</Project>
